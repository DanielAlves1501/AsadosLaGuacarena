{
  "hosting": {
    "redirects": [
      {
        "source": "/",
        "destination": "https://laguacarena.com",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "/robots.txt",
        "destination": "/robots.txt"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  }
}

{
  "hosting": {
    "redirects": [
      {
        "source": "/*",
        "destination": "https://laguacarena.com/:splat",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  }
}

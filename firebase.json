{
  "hosting": {
    "redirects": [
      {
        "source": "/*",
        "destination": "https://laguacarena.com/:splat",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "key": "Content-Type",
        "value": "application/xml"
      }
    ],
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  }
}
